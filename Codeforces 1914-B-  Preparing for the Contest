#include<bits/stdc++.h>
using namespace std;
#define yes cout<<"YES"<<"\n"
#define no cout<<"NO"<<"\n"
#define next "\n"
#define fori for(int i=0;i<n;i++)
#define forj for(int j=0;j<n;j++)
using ll = long long;
typedef long long ll;
const int N = (int)2e5+5;
vector<int>v[1005];
const int cnt = 0;

void solve()
{

   int n,m;
   cin>>n>>m;
   for(int i=n-m;i<=n;i++){
    cout<<i<<" ";
   }
   for(int i = n-m-1;i>=1;i--){
    cout<<i<<" ";
   }
   cout<<next;
}

int32_t main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL), cout.tie(NULL);
    int t = 1;
    cin>>t;
    while(t--)
    {
        solve();
    }

    return 0;
}
